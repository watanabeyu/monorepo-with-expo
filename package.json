{
  "name": "monorepo-with-expo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:packages": "turbo build --filter=./packages/* --filter=!./packages/typescript-config"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.12",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
