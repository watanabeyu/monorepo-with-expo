{
  "name": "expo-with-expo-router",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "cd ../../ && yarn build:packages",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo prebuild --clean && expo run:android",
    "ios": "expo prebuild --clean && expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build": "eas build --platform all",
    "build:ios": "eas build --auto-submit --platform ios",
    "build:android": "eas build --platform android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/native": "^6.0.2",
    "@repo/ui": "^0.0.0",
    "chance": "^1.1.12",
    "expo": "~51.0.26",
    "expo-build-properties": "^0.12.5",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.21",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "share-extension-expo-plugin": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/chance": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
